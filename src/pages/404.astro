---
import Layout from "../layouts/Layout.astro";
import ErrorHeader from "../components/ErrorHeader.astro";
import ContentLayout from "../components/ContentLayout.astro";
import TopNavbar from "../components/TopNavbar.astro";

const navigation = [
  { name: "Artykuły", url: "/" },
  { name: "Autorzy", url: "/autorzy" },
];
---

<Layout title="">
  <ContentLayout>
    <TopNavbar slot="header" items={navigation} />
    <ErrorHeader code="404" message="Strona nie została znaleziona" />
  </ContentLayout>
</Layout>
